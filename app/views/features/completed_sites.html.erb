<table id="" class="table table-striped table-bordered" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>Sheet</th>
      <th>Season</th>
      <th>Region</th>
      <th>Insula</th>
      <th>Entrance</th>
      <th>Edit</th>
    </tr>
  </thead>

  <tbody>
    <% @sites.each do |site| %>
      <tr>
        <td><%= site.sheet %></td>
        <td><%= site.season %></td>
        <td><%= site.region %></td>
        <td><%= site.insula %></td>
        <td><%= site.entrance %></td>
        <td><%= link_to "Edit", edit_feature_path(site), :class => "btn btn-primary" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>